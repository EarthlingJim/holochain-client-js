    "@types/tape": "^4.13.2",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "js-yaml": "^3.14.1",
    "rimraf": "^3.0.2",
    "tape": "^5.4.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
