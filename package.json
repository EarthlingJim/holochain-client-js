  "license": "CAL 1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holochain/holochain-client-js.git"
  },
  "keywords": [
    "holochain",
    "client",
    "conductor api"
  ],
  "bugs": {
    "url": "https://github.com/holochain/holochain-client-js/issues"
  },
